{
  "React Function Component": {
    "prefix": "rfc",
    "body": ["export const ${1:Component} = () => {", "  return (", "    <div>", "      ${2:content}", "    </div>", "  );", "};"],
    "description": "创建 React 函数组件"
  },
  "React Function Component Default Export": {
    "prefix": "rfce",
    "body": ["const ${1:Component} = () => {", "  return (", "    <div>", "      ${2:content}", "    </div>", "  );", "};", "", "export default ${1:Component};"],
    "description": "创建默认导出的 React 函数组件"
  },
  "React Arrow Function Component": {
    "prefix": "rafc",
    "body": ["import React from 'react';", "", "const ${1:Component} = () => {", "  return (", "    <div>", "      ${2:content}", "    </div>", "  );", "};", "", "export default ${1:Component};"],
    "description": "创建带 import 的箭头函数组件"
  },
  "useState Hook": {
    "prefix": "us",
    "body": ["const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState(${2:initialValue});"],
    "description": "创建 useState Hook"
  },
  "useEffect Hook": {
    "prefix": "ue",
    "body": ["useEffect(() => {", "  ${1:effect}", "  return () => {", "    ${2:cleanup}", "  };", "}, [${3:deps}]);"],
    "description": "创建 useEffect Hook"
  },
  "useCallback Hook": {
    "prefix": "ucb",
    "body": ["const ${1:callback} = useCallback(() => {", "  ${2:body}", "}, [${3:deps}]);"],
    "description": "创建 useCallback Hook"
  },
  "useMemo Hook": {
    "prefix": "um",
    "body": ["const ${1:value} = useMemo(() => {", "  return ${2:computation};", "}, [${3:deps}]);"],
    "description": "创建 useMemo Hook"
  },
  "useRef Hook": {
    "prefix": "ur",
    "body": ["const ${1:ref} = useRef(${2:null});"],
    "description": "创建 useRef Hook"
  },
  "useContext Hook": {
    "prefix": "uctx",
    "body": ["const ${1:value} = useContext(${2:Context});"],
    "description": "使用 useContext Hook"
  },
  "Custom Hook": {
    "prefix": "hook",
    "body": ["const use${1:Hook} = (${2:params}) => {", "  const [state, setState] = useState(${3:initialValue});", "", "  useEffect(() => {", "    ${4:effect}", "  }, [${5:deps}]);", "", "  return { state };", "};", "", "export default use${1:Hook};"],
    "description": "创建自定义 Hook"
  },
  "TypeScript Props Interface": {
    "prefix": "tsprops",
    "body": ["interface ${1:Component}Props {", "  ${2:prop}: ${3:type};", "}"],
    "description": "创建 TypeScript Props 接口"
  },
  "React Context": {
    "prefix": "ctx",
    "body": ["import React, { createContext, useContext, useState } from 'react';", "", "interface ${1:Name}ContextType {", "  ${2:value}: ${3:type};", "}", "", "const ${1:Name}Context = createContext<${1:Name}ContextType | undefined>(undefined);", "", "export const ${1:Name}Provider: React.FC<{ children: React.ReactNode }> = ({ children }) => {", "  const [${2:value}, set${2/(.*)/${1:/capitalize}/}] = useState<${3:type}>(${4:initial});", "", "  return (", "    <${1:Name}Context.Provider value={{ ${2:value} }}>", "      {children}", "    </${1:Name}Context.Provider>", "  );", "};", "", "export const use${1:Name} = () => {", "  const context = useContext(${1:Name}Context);", "  if (!context) throw new Error('use${1:Name} must be used within ${1:Name}Provider');", "  return context;", "};"],
    "description": "创建 React Context 完整模板"
  }
}
